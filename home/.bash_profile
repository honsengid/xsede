# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

PATH=$HOME/bin:$PATH
export PATH
MANPATH=$HOME/man:$MANPATH
PYTHONPATH=$HOME/lib/python:$PYTHONPATH
export PYTHONPATH
